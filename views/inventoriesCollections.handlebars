{{#if loggedIn}}
<div class="scrunchies">
  <section class="scrunchie">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">title</th>
        </tr>
      </thead>
      <tbody>
        {{#each collectionTitles}}
        <tr>
          <td scope="row">{{id}}</td>
          <td><a href="/collection/{{id}}">{{title}}</a></td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </section>
</div>
{{/if}}
